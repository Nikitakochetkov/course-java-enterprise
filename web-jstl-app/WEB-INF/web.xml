<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>AbilitiesServlet</servlet-name>
        <servlet-class>AbilitiesServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AbilitiesServlet</servlet-name>
        <url-pattern>/AbilitiesServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateHeroServlet</servlet-name>
        <servlet-class>CreateHeroServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CreateHeroServlet</servlet-name>
        <url-pattern>/CreateHeroServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HeroesServlet</servlet-name>
        <servlet-class>HeroesServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HeroesServlet</servlet-name>
        <url-pattern>/HeroesServlet</url-pattern>
    </servlet-mapping>


</web-app>